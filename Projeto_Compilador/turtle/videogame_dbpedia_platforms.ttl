:Platform,
        owl:NamedIndividual ;
    
:Abstract "Zeebo is a videogame platform family." ;
    
:Name "Zeebo" .

:Abstract a owl:DatatypeProperty ;
    rdfs:range xsd:string .

:ESRB_Rating a owl:DatatypeProperty ;
    rdfs:range xsd:string .

:Metacritic a owl:DatatypeProperty ;
    rdfs:range xsd:string .

:Name a owl:DatatypeProperty ;
    rdfs:range xsd:string .

:Playtime a owl:DatatypeProperty ;
    rdfs:range xsd:float .

:Release_Date a owl:DatatypeProperty ;
    rdfs:range xsd:string .

:appliesTo a owl:ObjectProperty ;
    owl:inverseOf :hasGenre .

:availableOn a owl:ObjectProperty ;
    rdfs:domain :Game ;
    rdfs:range :Platform ;
    owl:inverseOf :hosts .

:builtOn a owl:ObjectProperty ;
    rdfs:domain :Game ;
    rdfs:range :Engine ;
    owl:inverseOf :engineFor .

:developedBy a owl:ObjectProperty ;
    rdfs:domain :Game ;
    rdfs:range :Developer ;
    owl:inverseOf :hasDeveloped .

:developedDevice a owl:ObjectProperty ;
    rdfs:domain :Developer ;
    rdfs:range :Device ;
    owl:inverseOf :deviceDevelopedBy .

:developedEngine a owl:ObjectProperty ;
    owl:inverseOf :engineDevelopedBy .

:deviceForEngine a owl:ObjectProperty ;
    owl:inverseOf :engineMadeFor .

:includes a owl:ObjectProperty ;
    owl:inverseOf :partOf .

:isDeviceFrom a owl:ObjectProperty ;
    rdfs:domain :Device ;
    rdfs:range :Platform ;
    owl:inverseOf :isPlatformOf .

:Genre a owl:Class .

:Series a owl:Class .

:deviceDevelopedBy a owl:ObjectProperty .

:engineDevelopedBy a owl:ObjectProperty ;
    rdfs:domain :Engine ;
    rdfs:range :Developer .

:engineFor a owl:ObjectProperty .

:engineMadeFor a owl:ObjectProperty ;
    rdfs:domain :Engine ;
    rdfs:range :Device .

:hasDeveloped a owl:ObjectProperty .

:hasGenre a owl:ObjectProperty ;
    rdfs:domain :Game ;
    rdfs:range :Genre ;
    rdfs:subPropertyOf owl:topObjectProperty .

:hosts a owl:ObjectProperty .

:isPlatformOf a owl:ObjectProperty .

:partOf a owl:ObjectProperty ;
    rdfs:domain :Game ;
    rdfs:range :Series .

:Developer a owl:Class .

:Device a owl:Class ;
    rdfs:subClassOf :Platform .

:Engine a owl:Class .

:Platform a owl:Class .

:Game a owl:Class .

